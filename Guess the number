i=10

while(True):
    if i==10:
        i=i-1
    
    elif i==-1:     
                print("\nGameover. Better Luck Next Time") 

                break

  
    attempt=int(input())

    if attempt==20:
        print("are you a psychic. That's the currect answer")
        break
    elif attempt>20:
        print("it's greater than hidden number")
        print(i, "guesses left")
        i=i-1 

    elif attempt<20:
        print("it's smaller than hidden number")
        print(i, "guesses left")
        i=i-1 

        continue

    while i==-1:
        print("\nGameover. Better Luck Next Time")

        break
